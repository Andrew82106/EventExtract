    {
      "id": "text_28_node_4",
      "event_type": "Conflict",
      "event_subtype": "Attack",
      "event_sub_subtype": "Unspecified",
      "description": "The Revolutionary Guards blamed 'takfiri terrorists and mercenaries of the intelligence services of hegemonic powers' for the attack.",
